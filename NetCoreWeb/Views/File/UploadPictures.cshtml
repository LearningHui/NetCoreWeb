
@{
    ViewData["Title"] = "UploadPictures";
}

<h2>UploadPictures</h2>
<form asp-action="UploadPictures"
      asp-controller="File"
      method="post"
      enctype="multipart/form-data">
    <input type="file" name="files" multiple />
    <input type="submit" value="Upload Selected Files" />
</form>

